{
  "name": "voice-server",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch --env-file=../.env.development src/server.ts",
    "build": "bun build src/server.ts --target bun --outfile dist/server.js",
    "start": "bun --env-file=../.env.production ./dist/server.js"
  },
  "dependencies": {
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "license": "MIT"
}
