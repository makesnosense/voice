meta {
  name: all-user-devices
  type: http
  seq: 2
}

get {
  url: https://localhost:3003/api/devices/
  body: json
  auth: inherit
}

body:json {
  {
    "refreshToken": "{{voiceRefreshToken}}"
  }
}

script:post-response {
   const devices = res.getBody();
    
    if (devices.length > 0) {
      bru.setVar("jti", devices.at(-1).jti);
    }
  
}

settings {
  encodeUrl: true
  timeout: 0
}
